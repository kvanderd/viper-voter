<h2>Welcome <%= @user.username %></h2>
<h4>Latest Created Opinions</h4>
<div class="row">
<% @user.changes.each do |change| %>
<div class="col-md-2 intro-box text-center">
	<p><%= change.name %></p>
	<p><%= image_tag change.picture_url(:thumb) %></p>
	<p><%= change.description %></p>
</div>
<% end %>
</div>
